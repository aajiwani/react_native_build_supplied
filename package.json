{
  "name": "react_native_build_supplied",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest"
  },
  "dependencies": {
    "react": "16.6.3",
    "react-native": "0.57.8"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "pre-git": "^3.17.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  },
  "release": {
    "analyzeCommits": "simple-commit-message"
  },
  "config": {
    "pre-git": {
      "enabled": false,
      "msg-pattern": ".*[-]{2}build[-]android.*",
      "pre-push": [
        "cd android && ./gradlew assembleDebug",
        "cd ..",
        "mv android/app/build/outputs/apk/debug/app-debug.apk ci_builds/android",
        "git add .",
        "git commit -m 'New react container'"
      ]
    }
  }
}
